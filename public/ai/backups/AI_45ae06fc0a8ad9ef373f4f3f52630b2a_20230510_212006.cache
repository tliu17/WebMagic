```php
<?php
// Connect to the database
$db = DB();

// Fetch all users from the database
$query = "SELECT * FROM users";
$stmt = $db->prepare($query);
$stmt->execute();
$users = $stmt->fetchAll(PDO::FETCH_ASSOC);

// Display the list of users
echo "<h1>User List</h1>";
echo "<table border='1'>";
echo "<tr><th>ID</th><th>Username</th><th>Email</th><th>First Name</th><th>Last Name</th><th>Phone Number</th><th>Profile Image URL</th><th>Created At</th><th>Updated At</th></tr>";

foreach ($users as $user) {
    echo "<tr>";
    echo "<td>" . htmlspecialchars($user['user_id']) . "</td>";
    echo "<td>" . htmlspecialchars($user['username']) . "</td>";
    echo "<td>" . htmlspecialchars($user['email']) . "</td>";
    echo "<td>" . htmlspecialchars($user['first_name']) . "</td>";
    echo "<td>" . htmlspecialchars($user['last_name']) . "</td>";
    echo "<td>" . htmlspecialchars($user['phone_number']) . "</td>";
    echo "<td>" . htmlspecialchars($user['profile_image_url']) . "</td>";
    echo "<td>" . htmlspecialchars($user['created_at']) . "</td>";
    echo "<td>" . htmlspecialchars($user['updated_at']) . "</td>";
    echo "</tr>";
}

echo "</table>";
?>
```