<?php
// Database connection
require_once 'db.php';
$DB = DB();

// Fetch real estates data
$query = "SELECT * FROM real_estates";
$stmt = $DB->prepare($query);
$stmt->execute();
$result = $stmt->fetchAll(PDO::FETCH_ASSOC);

// Display real estates
echo "<h1>Real Estates List</h1>";
echo "<table border='1'>";
echo "<tr><th>ID</th><th>User ID</th><th>Type ID</th><th>Address ID</th><th>Title</th><th>Description</th><th>Price</th><th>Currency</th><th>Image URL</th><th>Created At</th><th>Updated At</th></tr>";

foreach ($result as $row) {
    echo "<tr>";
    echo "<td>" . $row['real_estate_id'] . "</td>";
    echo "<td>" . $row['user_id'] . "</td>";
    echo "<td>" . $row['real_estate_type_id'] . "</td>";
    echo "<td>" . $row['address_id'] . "</td>";
    echo "<td>" . $row['title'] . "</td>";
    echo "<td>" . $row['description'] . "</td>";
    echo "<td>" . $row['price'] . "</td>";
    echo "<td>" . $row['currency_code'] . "</td>";
    echo "<td>" . $row['image_url'] . "</td>";
    echo "<td>" . $row['created_at'] . "</td>";
    echo "<td>" . $row['updated_at'] . "</td>";
    echo "</tr>";
}

echo "</table>";
?>
